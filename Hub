-- Create a ScreenGui for the loading screen
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "LoadingScreen"
screenGui.Parent = game.CoreGui

-- Create a Frame for the loading screen
local loadingFrame = Instance.new("Frame")
loadingFrame.Size = UDim2.new(0, 350, 0, 250)
loadingFrame.Position = UDim2.new(0.5, -175, 0.5, -125)
loadingFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
loadingFrame.BorderSizePixel = 0
loadingFrame.Parent = screenGui

-- Add UICorner for rounded corners
local uiCorner = Instance.new("UICorner")
uiCorner.CornerRadius = UDim.new(0, 20)
uiCorner.Parent = loadingFrame

-- Add ImageLabel to loading screen
local loadingImage = Instance.new("ImageLabel")
loadingImage.Size = UDim2.new(1, 0, 1, 0)
loadingImage.BackgroundTransparency = 1
loadingImage.Image = "rbxassetid://11706064914"
loadingImage.Parent = loadingFrame

-- Create an Intro TextLabel
local introLabel = Instance.new("TextLabel")
introLabel.Text = "V3XADO HUB"
introLabel.Size = UDim2.new(1, 0, 0.3, 0)
introLabel.Position = UDim2.new(0, 0, 0.1, 0)
introLabel.BackgroundTransparency = 1
introLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
introLabel.Font = Enum.Font.GothamBold
introLabel.TextSize = 28
introLabel.TextStrokeTransparency = 0  -- Adding stroke effect
introLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
introLabel.Parent = loadingFrame

-- Create a Loading TextLabel with percentage
local loadingLabel = Instance.new("TextLabel")
loadingLabel.Text = "Loading... 0%"
loadingLabel.Size = UDim2.new(1, 0, 0.2, 0)
loadingLabel.Position = UDim2.new(0, 0, 0.5, 0)
loadingLabel.BackgroundTransparency = 1
loadingLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
loadingLabel.Font = Enum.Font.GothamBold
loadingLabel.TextSize = 20
loadingLabel.TextStrokeTransparency = 0  -- Adding stroke effect
loadingLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
loadingLabel.Parent = loadingFrame

-- Function to Fade In
local function fadeIn(element, duration)
    for i = 1, 0, -0.1 do
        element.TextTransparency = i
        wait(duration / 10)
    end
end

-- Function to Fade Out
local function fadeOut(element, duration)
    for i = 0, 1, 0.1 do
        element.TextTransparency = i
        wait(duration / 10)
    end
end

-- Function to Update Percentage
local function updateLoading(percent)
    loadingLabel.Text = "Loading... " .. tostring(percent) .. "%"
end

-- Intro animation
fadeIn(introLabel, 1)  -- Fade in the intro text
wait(1)  -- Hold the intro text

-- Transition to loading
fadeIn(loadingLabel, 1)  -- Fade in the loading text

-- Simulate loading time with percentage
for i = 1, 100 do
    updateLoading(i)
    wait(0.05)  -- Adjust the speed of the percentage increment
end

wait(1)  -- Hold the 100% message for a moment

-- Transition to remove loading screen
fadeOut(loadingLabel, 1)  -- Fade out the loading text
fadeOut(introLabel, 1)  -- Fade out the intro text
wait(1)
loadingFrame:Destroy()  -- Remove the loading screen

